<div class="row" style="max-width: 1400px;">
  <% page.posts.each(function(post, i){ %>
    <% var content = strip_html(post.content) %>
    <div class="col-6 col-6-md col-4-lg card-<%- i %>" >
      <div class="card">
        <header>
          <a rel="prefetch" href="<%- url_for(post.path) %>">
            <figure>
              <img src='/assets/img/<% if (post.img) {%><%- post.img %><%}else{%>serving-vector-tiles.jpg<%}%>' style="object-fit:cover;object-position:center;width:100%;height:100%;" loading="lazy" alt="Fun Map Photo">
            </figure>
          </a>
        </header>
        <section>
          <%- partial('post/header', {post: post, index: true}) %>
        </section>
      </div>
    </div>
    <%})%>
