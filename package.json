{
  "name": "getbounds",
  "version": "3.0.0",
  "private": true,
  "hexo": {
    "version": "6.0.0"
  },
  "scripts": {
    "build": "node github && hexo clean && hexo generate",
    "build:watch": "node github && hexo clean && hexo generate --watch",
    "css": "parcel build public/style.css --dist-dir public/",
    "gen": "hexo generate",
    "clean": "hexo clean",
    "deploy": "hexo deploy",
    "serve": "hexo server",
    "start": "node github && hexo server",
    "dev": "node github && hexo server --draft",
    "drafts": "hexo generate --draft --watch & live-server --port=4000 public",
    "avif": "avif --input='public/assets/img/*.{jpg, png}' --speed=0 --quality=80"
  },
  "dependencies": {
    "avif": "^0.1.0",
    "bluebird": "^3.7.2",
    "chota": "^0.8.0",
    "hexo": "^6.0.0",
    "hexo-admin": "^2.3.0",
    "hexo-excerpt": "^1.2.1",
    "hexo-generator-archive": "^1.0.0",
    "hexo-generator-category": "^1.0.0",
    "hexo-generator-feed": "^3.0.0",
    "hexo-generator-index": "^2.0.0",
    "hexo-generator-json-content": "^4.2.3",
    "hexo-generator-seo-friendly-sitemap": "0.2.1",
    "hexo-generator-tag": "^1.0.0",
    "hexo-log": "^3.0.0",
    "hexo-renderer-ejs": "^2.0.0",
    "hexo-renderer-marked": "^4.1.0",
    "hexo-renderer-stylus": "^2.0.1",
    "hexo-server": "^3.0.0",
    "hexo-util": "^2.5.0",
    "install": "^0.13.0",
    "lodash.assign": "^4.2.0",
    "micromatch": "^4.0.4",
    "node-fetch": "^2.6.1",
    "npm": "^9.2.0",
    "nunjucks": "^3.2.3",
    "parcel": "^2.2.1",
    "postcss": "^8.4.5",
    "sharp": "^0.30.5",
    "stream-to-array": "^2.3.0",
    "stream-to-promise": "^3.0.0",
    "striptags": "^3.2.0"
  },
  "devDependencies": {
    "bower": "^1.8.13",
    "cssnano": "^5.0.15",
    "hexo-browsersync": "^0.3.0"
  }
}
