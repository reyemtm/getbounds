<meta charset="UTF-8">
<meta name="viewport" content="width=device-width, initial-scale=1.0">
<!-- <link rel="manifest" href="/manifest.json"> -->
<meta name="theme-color" content="<%- config.color %>" />
<title><%- config.title %> </title>
<link rel="shortcut icon" type="image/x-icon" href="/favicon.ico">
<!-- hexo opengraph -->

<%- open_graph({
  twitter_id: config.twitter
}) %>
<meta name="twitter:site" content="@<%= config.twitter %>">
<meta name="twitter:title" content="<%= page.title %>">
<meta name="twitter:image" content="<%- config.url %>/assets/img/<% if (page.img) {%>twitter_square_<%= (page.img).replace(".png", ".webp").replace(".jpg", ".webp" ) %><%}else{%><%- config.defaultimage %><%}%>">


<meta property="og:image" content="<%- config.url %>/assets/img/<% if (page.img) {%>twitter_square_<%= (page.img).replace(".png", ".webp").replace(".jpg", ".webp" ) %><%}else{%><%- config.defaultimage %><%}%>">
<% if (page.img && page.img_width && page.img_height) {%><meta property="og:image:width" content="<%= page.img_width %>">
<meta property="og:image:height" content="<%= page.img_height %>">
<%}%>
<% if (config.favicon){ %><link rel="icon" href="<%- config.favicon %>"><% } %>
<!-- canonical url tag-->
<link rel="canonical" href="<% if (page.canonical) {%><%= page.canonical %><%}else{%><%= config.url %><%= url_for(page.path) %><%}%>">

<!-- end opengraph -->
<% if (is_home()) {%><link rel="stylesheet" href="/landing.css"><%}else{%>
<link rel="stylesheet" href="/post.css">
<link rel="stylesheet" href="/stackoverflow-dark.css">
<link rel="stylesheet" href="https://rsms.me/inter/inter.css">
<%}%>
<!-- scripts and styles in page.header -->
<% if (page.head) {%>
<%- page.head %>
<%}%>


<!-- page.style page.css -->
<% if (page.style) {%>
<style>
<%- page.style %>
</style>
<%}%>

<% if (page.css) {%>
<style>
<%- page.css %>
</style>
<%}%>
</head>
